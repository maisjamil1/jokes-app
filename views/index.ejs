            <p><%=val.punchline%></p>
            <form action="/addtolist" method="POST">
            <input type="text" name="jokenumber" value="<%=val.jokenumber%>">
            <input type="text" name="typee" value="<%=val.typee%>">
            <input type="text" name="setup" value="<%=val.setup%>">
            <input type="text" name="punchline" value="<%=val.punchline%>">
            <button> add to jokes list</button>
            
            
            </form>
        </section>

        <%})%>







    </div>
